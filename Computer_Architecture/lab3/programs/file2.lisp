; program "guess the color"

(function check-color (input-color true-color)
  (if (not (equal input-color true-color))
      (group
        (print "Вы не угадали, попробуйте ещё раз")
        false)
      (group
        (print "It's right answer!")
        true)))

(let ((input-color (input))
      (true-color "red"))
(check-color input-color true-color)

